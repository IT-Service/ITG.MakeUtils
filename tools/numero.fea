languagesystem DFLT dflt;
languagesystem latn ENG;
languagesystem latn RUS;

feature liga {
    sub N ordmasculine by uni2116;
    sub N degree by uni2116;
} liga;

feature liga {
	script latn;
	language ENG exclude_dflt;
    sub N o period by uni2116;
} liga;

feature liga {
	script latn;
	language RUS exclude_dflt;
    sub N o period by uni2116;
} liga;
