    verification_stamp_rhombus_id_h_size scalefont setfont
    0 0 moveto
      verification_stamp_id
      string_center_middle
    true charpath
  setmatrix

  % знак СТ
  0 -6.0 moveto
  currentpoint translate
  verification_stamp_rhombus_rst_size dup scale
  ust uappend

  % восстановливаем систему координат
  setmatrix

  end
} bind def

