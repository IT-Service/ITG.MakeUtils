  currentpoint translate
  scale_k scale_k scale
  45 rotate

  verification_stamp_rhombus_h_size 2 div  dup  translate

  % рамка
  matrix currentmatrix
    45 rotate
      verification_stamp_rhombus_h_size
      dup
      verification_stamp_rhombus_thickness
      verification_stamp_rhombus_radius
    centered_frame
  setmatrix

  % индивидуальный знак поверителя
    verification_stamp_sign 0 ne
    {
      matrix currentmatrix
