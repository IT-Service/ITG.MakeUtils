/areverse  %  [a b ... z] --> [z ... b a]
{
  [ exch
    aload
    length
    2  1  3 -1 roll
      {
        -1 roll
      }
    for
  ]
} bind def

/aconcatenate  %  [(a) (b) ... (z)] --> (ab...z)
{
  0  1 index  { length add } forall  string
  0 3 2 roll
    {
      3 copy putinterval
      length add
    }
  forall pop  
} bind def

