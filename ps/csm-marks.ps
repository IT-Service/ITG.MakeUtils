% размещение объектов в шахматном порядке
/align_staggered
{
  6 dict begin
  /ItemSize exch def % размеры объектов [x y]
  /AreaBBox exch def % координаты области для размещения объектов [x1 y1 x2 y2]
  /ItemX  ItemSize aload pop pop  def
  /ItemY  ItemSize aload pop exch pop  def
  /BBoxX  AreaBBox aload pop pop exch pop sub neg  def
  /BBoxY  AreaBBox aload pop exch pop sub neg exch pop  def
  gsave
  
  AreaBBox aload pop pop pop  translate
  
  0 % координата X начала первой строки
    0  ItemY  BBoxY ItemY sub
    {
        exch dup  3 1 roll
        ItemX
        BBoxX ItemX sub
        {
          exch dup  3 1 roll
          newpath
          moveto
          2017 (IV) (СП) (а) 18.0 mm verification_stamp_path
          fill
        }
      for
      pop
      % изменяем координату X начала строки
      ItemX 2 div  sub
      dup  0 lt { ItemX add } if
    }
  for
  pop

  grestore
  end
} bind def

% размещение объектов по сетке
/align_to_grid
{
  6 dict begin
  /ItemSize exch def % размеры объектов [x y]
  /AreaBBox exch def % координаты области для размещения объектов [x1 y1 x2 y2]
  /ItemX  ItemSize aload pop pop  def
  /ItemY  ItemSize aload pop exch pop  def
  /BBoxX  AreaBBox aload pop pop exch pop sub neg  def
  /BBoxY  AreaBBox aload pop exch pop sub neg exch pop  def
  gsave
  
  AreaBBox aload pop pop pop  translate
  
    0  ItemY  BBoxY ItemY sub
    {
        0  ItemX  BBoxX ItemX sub
        {
          exch dup  3 1 roll
          newpath
          moveto
          2017 (IV) (СП) (а) 18.0 mm verification_stamp_path
          fill
        }
      for
    }
  for

  grestore
  end
} bind def

